{
    "name": "live-chat-overlay",
    "version": "2.13.1",
    "description": "Web Extension for Overlay Live Chat on Youtube",
    "main": "dist/content-script.js",
    "repository": "git@github.com:thwonghin/live-chat-overlay.git",
    "author": "Hin Wong <thwong.henry@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=16.9.1"
    },
    "scripts": {
        "test:unit": "tsc -p tests && jest tests/unit",
        "lint": "yarn lint:src && yarn lint:test && yarn lint:style",
        "lint:script": "eslint --ext ts,tsx",
        "lint:src": "yarn lint:script src webpack/src",
        "lint:test": "yarn lint:script tests",
        "lint:style": "stylelint './src/**/*.tsx'",
        "lint:fix": "yarn lint:src --fix && yarn lint:test --fix",
        "clean": "rm -rf dist .storybook/dist webpack/dist webpack/tsconfig.tsbuildinfo",
        "build": "tsc -p webpack && webpack --config webpack/dist/webpack.config.js",
        "build:prod": "yarn build --env production",
        "start": "yarn build --env development --watch",
        "postversion": "npx conventional-changelog-cli -p live-chat-overlay -i CHANGELOG.md -s && git add CHANGELOG.md",
        "version:set": "dot-json dist/manifest.json version",
        "storybook": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && start-storybook -c .storybook/dist/",
        "storybook:build": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && build-storybook -c .storybook/dist/ --quiet",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "postinstall": "patch-package"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@storybook/builder-webpack5": "^6.3.8",
        "@storybook/manager-webpack5": "^6.3.8",
        "@storybook/react": "^6.3.8",
        "@swc/core": "^1.2.89",
        "@swc/jest": "^0.2.3",
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/jest": "^27.0.2",
        "@types/json-schema": "^7.0.9",
        "@types/lodash-es": "^4.17.5",
        "@types/node": "^16.9.6",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/tapable": "^2.2.2",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "browserslist": "^4.17.1",
        "copy-webpack-plugin": "^9.0.1",
        "dot-json": "^1.2.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-xo": "^0.38.0",
        "eslint-config-xo-react": "^0.25.0",
        "eslint-config-xo-typescript": "^0.44.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.4.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.3.3",
        "husky": "7.0.2",
        "jest": "^27.2.1",
        "lodash": "^4.17.21",
        "pinst": "^2.1.6",
        "prettier": "^2.4.1",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recess-order": "^2.5.0",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-order": "^4.1.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-processor-styled-components": "^1.10.0",
        "swc-loader": "^0.1.15",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typescript": "^4.4.3",
        "webpack": "^5.53.0",
        "webpack-cli": "^4.8.0"
    },
    "browserslist": [
        "Chrome >= 88"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.15",
        "@mui/material": "^5.0.1",
        "@mui/styled-engine-sc": "^5.0.0",
        "@reduxjs/toolkit": "^1.6.1",
        "@types/styled-components": "^5.1.14",
        "core-js": "3.18.0",
        "formik": "^2.2.9",
        "lodash-es": "^4.17.21",
        "patch-package": "^6.4.7",
        "postinstall-postinstall": "^2.1.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.5",
        "redux": "^4.1.1",
        "styled-components": "^5.3.1",
        "type-fest": "^2.3.4",
        "use-debounce": "^7.0.0",
        "webextension-polyfill-ts": "^0.26.0"
    },
    "resolutions": {
        "stylelint-config-rational-order/stylelint": "^13.12.0",
        "trim": "^1.0.1",
        "browserslist": "^4.17.0",
        "glob-parent": "^5.1.2",
        "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
    }
}
