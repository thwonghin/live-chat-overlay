{
    "name": "live-chat-overlay",
    "version": "2.10.1",
    "description": "Web Extension for Overlay Live Chat on Youtube",
    "main": "dist/content-script.js",
    "repository": "git@github.com:thwonghin/live-chat-overlay.git",
    "author": "Hin Wong <thwong.henry@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=14.16.1"
    },
    "scripts": {
        "test:unit": "tsc -p tests && jest tests/unit",
        "lint": "yarn lint:src && yarn lint:test && yarn lint:style",
        "lint:script": "eslint --ext ts,tsx",
        "lint:src": "yarn lint:script src",
        "lint:test": "yarn lint:script tests",
        "lint:style": "stylelint src/**/*.scss",
        "lint:fix": "xo --fix && yarn lint:style --fix",
        "clean": "rm -rf dist .storybook/dist webpack/dist webpack/tsconfig.tsbuildinfo src/**/*.scss.d.ts",
        "build": "tsc -p webpack && webpack --config webpack/dist/webpack.config.js",
        "build:prod": "yarn build --env production",
        "build:release": "yarn build --env release",
        "start": "yarn build --env development --watch",
        "postversion": "npx conventional-changelog-cli -p live-chat-overlay -i CHANGELOG.md -s && git add CHANGELOG.md",
        "version:set": "dot-json dist/manifest.json version",
        "storybook": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && start-storybook -c .storybook/dist/",
        "storybook:build": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && build-storybook -c .storybook/dist/ --quiet",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "@storybook/builder-webpack5": "^6.2.8",
        "@storybook/react": "^6.2.8",
        "@swc/core": "^1.2.52",
        "@types/classnames": "^2.2.11",
        "@types/copy-webpack-plugin": "^6.4.1",
        "@types/cssnano": "^4.0.0",
        "@types/jest": "^26.0.22",
        "@types/json-schema": "^7.0.7",
        "@types/lodash-es": "^4.17.4",
        "@types/mini-css-extract-plugin": "^1.4.1",
        "@types/node": "^14.14.41",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-redux": "^7.1.16",
        "@types/stylelint": "^9.10.1",
        "@types/tapable": "^2.2.2",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "browserslist": "^4.16.4",
        "copy-webpack-plugin": "^8.1.1",
        "css-loader": "^5.2.2",
        "cssnano": "^5.0.1",
        "dot-json": "^1.2.2",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-config-xo": "^0.36.0",
        "eslint-config-xo-react": "^0.25.0",
        "eslint-config-xo-typescript": "xojs/eslint-config-xo-typescript.git#0fac85ed43ffdcc9986fa9f7374963093c9be141",
        "eslint-plugin-jest": "^24.3.5",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "patch:fork-ts-checker-webpack-plugin@^6.2.1#./patches/fork-ts-checker-webpack-plugin+6.2.1.patch",
        "husky": "6.0.0",
        "jest": "^26.6.3",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "^1.4.1",
        "pinst": "^2.1.6",
        "postcss": "^8.2.10",
        "postcss-html": "^0.36.0",
        "postcss-loader": "^5.2.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-syntax": "^0.36.2",
        "prettier": "^2.2.1",
        "sass": "^1.32.10",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.12.0",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-xo-scss": "^0.14.0",
        "stylelint-order": "^4.1.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^3.19.0",
        "swc-loader": "^0.1.13",
        "ts-jest": "^26.5.5",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typed-css-modules-webpack-plugin": "patch:typed-css-modules-webpack-plugin@^0.2.0#./patches/typed-css-modules-webpack-plugin+0.2.0.patch",
        "typescript": "^4.2.4",
        "webpack": "^5.33.2",
        "webpack-cli": "^4.6.0"
    },
    "browserslist": [
        "Chrome >= 74"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@material-ui/core": "^4.11.3",
        "@reduxjs/toolkit": "^1.5.1",
        "classnames": "^2.3.1",
        "core-js": "3.10.1",
        "formik": "^2.2.6",
        "jss": "^10.6.0",
        "lodash-es": "^4.17.21",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.3",
        "redux": "^4.0.5",
        "type-fest": "^1.0.2",
        "use-debounce": "^6.0.1",
        "webextension-polyfill-ts": "^0.25.0"
    },
    "resolutions": {
        "stylelint-config-rational-order/stylelint": "^13.12.0"
    }
}
