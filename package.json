{
    "name": "live-chat-overlay",
    "version": "2.9.0",
    "description": "Web Extension for Overlay Live Chat on Youtube",
    "main": "dist/content-script.js",
    "repository": "git@github.com:thwonghin/live-chat-overlay.git",
    "author": "Hin Wong <thwong.henry@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": ">=14.15.4"
    },
    "scripts": {
        "test:unit": "jest tests/unit",
        "lint": "yarn lint:src && yarn lint:test && yarn lint:style",
        "lint:script": "xo",
        "lint:src": "yarn lint:script src",
        "lint:test": "yarn lint:script tests",
        "lint:style": "stylelint src/**/*.scss",
        "lint:fix": "xo --fix && yarn lint:style --fix",
        "clean": "rm -rf dist .storybook/dist webpack/dist webpack/tsconfig.tsbuildinfo",
        "build": "tsc -p webpack && webpack --config webpack/dist/webpack.config.js",
        "build:prod": "yarn build --env production",
        "build:release": "yarn build --env release",
        "start": "yarn build --env development --watch",
        "version": "npx conventional-changelog-cli -p live-chat-overlay -i CHANGELOG.md -s && git add CHANGELOG.md",
        "version:set": "dot-json dist/manifest.json version",
        "storybook": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && start-storybook -c .storybook/dist/",
        "storybook:build": "tsc -p webpack && tsc -p .storybook && cp .storybook/src/preview-head.html .storybook/dist/ && build-storybook -c .storybook/dist/ --quiet",
        "postinstall": "patch-package",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/plugin-proposal-class-properties": "^7.12.13",
        "@babel/preset-env": "^7.12.16",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.12.16",
        "@babel/runtime": "^7.12.13",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@storybook/react": "^6.1.18",
        "@types/classnames": "^2.2.11",
        "@types/copy-webpack-plugin": "^6.4.0",
        "@types/cssnano": "^4.0.0",
        "@types/jest": "^26.0.20",
        "@types/lodash-es": "^4.17.4",
        "@types/mini-css-extract-plugin": "^1.2.2",
        "@types/node": "^14.14.28",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "@types/react-redux": "^7.1.16",
        "@types/stylelint": "^9.10.1",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.0.2",
        "cssnano": "^4.1.10",
        "dot-json": "^1.2.1",
        "eslint-config-xo-react": "^0.23.0",
        "eslint-config-xo-typescript": "^0.37.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "husky": "5",
        "jest": "^26.6.3",
        "mini-css-extract-plugin": "^1.3.7",
        "pinst": "^2.1.4",
        "postcss-loader": "^4.2.0",
        "postcss-preset-env": "^6.7.0",
        "sass": "^1.32.7",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.10.0",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.19.0",
        "ts-jest": "^26.5.1",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typed-css-modules-webpack-plugin": "^0.2.0",
        "typescript": "^4.1.5",
        "webpack": "^5.22.0",
        "webpack-cli": "^4.5.0",
        "xo": "^0.37.1"
    },
    "browserslist": [
        "Chrome >= 74"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@material-ui/core": "^4.11.3",
        "@reduxjs/toolkit": "^1.5.0",
        "classnames": "^2.2.6",
        "core-js": "3.9.0",
        "formik": "^2.2.6",
        "jss": "^10.5.1",
        "lodash-es": "^4.17.20",
        "patch-package": "^6.2.2",
        "postinstall-postinstall": "^2.1.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "type-fest": "^0.21.1",
        "use-debounce": "^5.2.0",
        "webextension-polyfill-ts": "^0.25.0"
    },
    "resolutions": {
        "stylelint-config-rational-order/stylelint": "^13.10.0"
    }
}
