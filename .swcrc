{
    "test": ".*.(j|t)sx?$",
    "jsc": {
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        },
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false,
            "dynamicImport": false
        },
        "experimental": {
            "plugins": [
                [
                    "@swc/plugin-styled-components",
                    {
                        "displayName": true
                    }
                ]
            ]
        }
    },
    "env": {
        "mode": "entry",
        "coreJs": 3
    }
}
